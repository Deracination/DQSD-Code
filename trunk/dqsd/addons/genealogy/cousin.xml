  <script src="addons/genealogy/genealogy_lib.js"></script>
  <script><![CDATA[
    function cousin(q)
    {
      if( nullArgs("cousin", q) )
        return false;

      var search_url = "http://www.cousinconnect.com/locs/par/1/page/3/";
      var links = genealogy_get_known_page_links(search_url);
      var search_place_num = genealogy_lookup_place(q);
      if (search_place_num >= 0) {
        openSearchWindow(links[search_place_num]);
      } else {
        // assume it's a surname and do a search
	document.cousinf.surname.value = genealogy_parse_surname(q);
	submitForm(cousinf);
      }
    }
    ]]>
  </script>
